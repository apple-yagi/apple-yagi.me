{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:css && pnpm build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/generated.css",
    "deploy": "pnpm build && wrangler publish",
    "publish": "npm run build && wrangle publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/generated.css",
    "dev": "pnpm run build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "api:build": "aspida"
  },
  "dependencies": {
    "@aspida/fetch": "1.11.0",
    "@remix-run/cloudflare": "1.7.4",
    "@remix-run/cloudflare-workers": "1.7.4",
    "@remix-run/react": "1.7.4",
    "aspida": "1.11.0",
    "cross-env": "7.0.3",
    "microcms-js-sdk": "2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@remix-run/dev": "1.7.4",
    "@remix-run/eslint-config": "1.7.4",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/xml2js": "^0.4.11",
    "autoprefixer": "10.4.13",
    "concurrently": "7.5.0",
    "eslint": "8.26.0",
    "miniflare": "2.11.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.18",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4",
    "wrangler": "2.1.14"
  },
  "engines": {
    "node": ">=16.13"
  }
}
