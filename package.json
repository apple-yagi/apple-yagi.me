{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks",
    "build": "pnpm build:css && pnpm build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/generated.css",
    "deploy": "pnpm build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/generated.css",
    "dev": "pnpm run build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "api:build": "aspida",
    "typecheck": "tsc --noEmit",
    "lint": "eslint app --ext ts,tsx",
    "lint:fix": "eslint app --fix --ext ts,tsx",
    "format": "rome format app",
    "format:write": "rome format app --write"
  },
  "dependencies": {
    "@aspida/fetch": "1.11.0",
    "@remix-run/cloudflare": "1.9.0",
    "@remix-run/cloudflare-workers": "1.9.0",
    "@remix-run/react": "1.9.0",
    "aspida": "1.11.0",
    "cross-env": "7.0.3",
    "microcms-js-sdk": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.1",
    "@remix-run/dev": "1.9.0",
    "@remix-run/eslint-config": "1.9.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/xml2js": "0.4.11",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.31.0",
    "eslint-plugin-react": "7.31.11",
    "lint-staged": "13.1.0",
    "miniflare": "2.11.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "rome": "11.0.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "wrangler": "2.6.2",
    "xo": "0.53.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
